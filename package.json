{
  "name": "jefri-jiffies",
  "title": "JEFRi Jiffies",
  "version": "0.1.2",
  "description": "JEFRi Jiffies: minimal helpers & polyfills.",
  "homepage": "http://www.jefri.org",
  "bugs": "https://github.com/jefri/jefri-jiffies/issues",
  "author": {
    "name": "David Souther",
    "email": "davidsouther@gmail.com",
    "url": "http://davidsouther.com"
  },
  "files": [
    "lib/",
    "src/",
    "package.json"
  ],
  "main": "./src",
  "scripts": {
    "build": "tsc -p src ; tsc -p test ; webpack ./dist/index.js ./lib/jiffies.js ; webpack test/**/*.js test/tests.js",
    "pretest": "npm run build",
    "test": "mocha test/**/*.js ; karma start karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jefri/jefri-runtime"
  },
  "keywords": [
    "ef",
    "entity",
    "entity framework"
  ],
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "browser": {
    "./dist/request/server.js": "./dist/request/browser.js",
    "./dist/random/server.js": "./dist/random/browser.js",
    "./dist": "./lib/jiffies.js"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "glob": "^6.0.1",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-mocha": "^0.2.1",
    "karma-spec-reporter": "0.0.22",
    "mocha": "^2.3.4",
    "typescript": "^1.6.2",
    "webpack": "^1.12.8"
  },
  "dependencies": {
    "request": "^2.53.0"
  }
}
