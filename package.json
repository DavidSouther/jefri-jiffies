{
  "name": "jefri-jiffies",
  "title": "JEFRi Jiffies",
  "version": "0.1.3-pre3",
  "description": "JEFRi Jiffies: minimal helpers & polyfills.",
  "homepage": "http://www.jefri.org",
  "bugs": "https://github.com/jefri/jefri-jiffies/issues",
  "author": {
    "name": "David Souther",
    "email": "davidsouther@gmail.com",
    "url": "http://davidsouther.com"
  },
  "files": [
    "lib/",
    "src/",
    "dist/",
    "package.json"
  ],
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "format": "clang-format --style=google -i {src,test}/**/*.ts",
    "build": "rm -rf build/ lib/ && tsc && cp -a ./build/src ./lib && webpack ./build/test/**/*.js ./build/tests.js",
    "pretest": "npm run format ; npm run build",
    "test": "mocha ./build/test/mocha/**/*.js && karma start karma.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jefri/jefri-runtime"
  },
  "keywords": [
    "ef",
    "entity",
    "entity framework"
  ],
  "licenses": [
    {
      "type": "MIT"
    }
  ],
  "devDependencies": {
    "chai": "^3.4",
    "karma": "^0.13",
    "karma-chai": "^0.1",
    "karma-chrome-launcher": "^0.2",
    "karma-mocha": "^0.2",
    "karma-spec-reporter": "^0.0",
    "mocha": "^2.3",
    "superagent-mock": "^1.9",
    "typescript": "^1.6",
    "webpack": "^1.12"
  },
  "dependencies": {
    "superagent": "^1.4"
  }
}
